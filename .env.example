# ==========================================
# METATRADER 5 CONFIGURATION
# ==========================================
# Your Trading Account Login (Demo or Real)
MT5_LOGIN=12345678

# Your Trading Account Password
MT5_PASSWORD=your_password_here

# Broker Server Name (e.g., XP-MT5, MetaQuotes-Demo)
MT5_SERVER=MetaQuotes-Demo

# Path to the terminal64.exe executable
# Default example: "C:\Program Files\MetaTrader 5\terminal64.exe"
MT5_PATH="C:\Program Files\MetaTrader 5\terminal64.exe"

# ==========================================
# TRADING SETTINGS
# ==========================================
# Default asset/symbol to trade
SYMBOL=EURUSD

# Chart Timeframe (M1, M5, H1, D1)
TIMEFRAME=M5

# Trade Volume (Lots)
VOLUME=0.01

# ==========================================
# TELEGRAM BOT CONFIGURATION
# ==========================================
# Token obtained from @BotFather
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz

# Your numeric Chat ID (get it from @userinfobot)
TELEGRAM_CHAT_ID=987654321